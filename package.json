{
  "name": "express-with-nodemon-browsersync",
  "version": "1.1.0",
  "private": true,
  "description": "Auto restart ExpressJS server and auto reload browser on file changes.",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "nodemon server.js",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "generate-key-file": "node generate-key.js"
  },
  "engines": {
    "node": "12.18.0"
  },
  "dependencies": {
    "@google-cloud/vision": "^4.1.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.1.0",
    "ejs": "^2.6.1",
    "express": "4.14.0",
    "ramda": "^0.26.0"
  },
  "devDependencies": {
    "browser-sync": "2.12.8",
    "cross-env": "1.0.8",
    "nodemon": "^1.19.4"
  }
}
